package LOOPS;

import java.util.Scanner;

public class whileloop 
{
    public static void main(String[] args)
    {        int i,n,t;
        Scanner s=new Scanner(System.in);
        System.out.println("Enter a Number");
        n=s.nextInt();
        i=1;
        while(i<=10)
        {
            t=n*i;
            System.out.println(t);
            i=i+1;
        }
       
    }
    
}
#This is Java Program
